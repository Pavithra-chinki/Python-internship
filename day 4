l1 = [1, 3, 5, 6]
l1.append(7)
print(l1)
l1.remove(3)
print(l1)
l2= [20, 60, 50, 35, 90]
a=max(l2)
print(a)
b=min(l2)
print(b)


tuple=("apple","cherry","banana","grapes","orange")
reversedtuple=tuple[::-1]
print(reversedtuple)


mytuple = (123, 'jonas', 'sara', 'nancy', 564);
mylist = list(mytuple)
print("List elements : ", mylist)

